#include <iostream>
using namespace std;
int main() {

   float gastofamilia[5];
   for (int i = 0; i < 5; i++){
    cout << "digite os gastos da familia " << i + 1 << endl;
    cin >> gastofamilia[i];
   }

   float media = 0;
   for (int i = 0; i < 5; i ++){
    media += gastofamilia[i];
   }

   media = media / 5;

   cout << "Media de gastos : " << media << endl;
    
   return 0; 
}
